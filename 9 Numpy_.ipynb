    "##  9.5: Singular Values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "n = 200\n",
    "C = np.random.rand(n,n)\n",
    "p = 0.8\n",
    "C = (C < p) * 1.0\n",
    "largest_sin = np.linalg.norm(C, 2)\n",
    "print largest_sin\n",
    "print 'We find that n * p approximately equals largest singular value'"
   ]
  }
